{
  "name": "dev-showcase-ai",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx vite --port 3000 --host localhost --strict-port --force",
    "dev:frontend": "npx vite --port 3000 --host localhost --strict-port --force",
    "dev:frontend:custom": "node start-frontend-only.js",
    "dev:backend": "node server/server.js",
    "dev:backend:custom": "node start-backend-only.js",
    "dev:both": "node start-servers.js",
    "dev:both:clean": "node start-both-clean.js",
    "dev:smart": "node start-smart.js",
    "restart": "node start-smart.js",
    "start:backend": "cd server && node server.js",
    "start:frontend": "npm run dev",
    "start:frontend:custom": "node start-frontend-only.js",
    "build": "vite build",
    "start": "vite preview --port 3004",
    "start:prod": "serve -s dist -l 3000",
    "server": "vite --port 3000",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:integration": "node scripts/test-e2e.mjs",
    "test:performance": "lighthouse-ci",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:all": "npm run test:ci && npm run test:integration && npm run test:e2e && npm run test:performance && npm run test:accessibility",
    "analyze": "cross-env ANALYZE=true npm run build",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "prepare": "echo 'Skipping husky install'",
    "auto-git": "node auto-git.js",
    "auto-git:start": "node auto-git.js start",
    "auto-git:commit": "node auto-git.js commit",
    "auto-git:status": "node auto-git.js status",
    "git:auto": "node auto-git.js start"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^3.3.4",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.8.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@react-three/drei": "^9.88.13",
    "@react-three/fiber": "^8.15.19",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query": "^5.17.19",
    "@types/d3": "^7.4.3",
    "bcryptjs": "^3.0.2",
    "bullmq": "^4.18.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.0",
    "connect-redis": "^9.0.0",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "framer-motion": "^10.18.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jose": "^5.2.0",
    "lucide-react": "^0.312.0",
    "mongodb": "^6.3.0",
    "mongoose": "^8.19.0",
    "nanoid": "^5.1.6",
    "next": "14.1.0",
    "next-auth": "^4.24.5",
    "next-themes": "^0.4.6",
    "node-fetch": "^3.3.2",
    "openai": "^4.26.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-resizable-panels": "^0.0.63",
    "react-router-dom": "^7.9.4",
    "react-spring": "^10.0.3",
    "recharts": "^2.10.3",
    "redis": "^5.9.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.160.1",
    "zod": "^3.22.4",
    "zustand": "^4.5.0",
    "serve": "^14.2.5"
  },
  "devDependencies": {
    "@axe-core/react": "^4.8.2",
    "@cypress/code-coverage": "^3.12.15",
    "@lhci/cli": "^0.12.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/three": "^0.160.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.17",
    "axe-core": "^4.8.3",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.2",
    "cypress-axe": "^1.4.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^11.4.0",
    "lint-staged": "^15.2.0",
    "msw": "^2.0.11",
    "postcss": "^8.4.33",
    "prisma": "^5.8.1",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^6.3.6",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@/components/(.*)$": "<rootDir>/src/components/$1",
      "^@/lib/(.*)$": "<rootDir>/src/lib/$1",
      "^@/pages/(.*)$": "<rootDir>/src/pages/$1",
      "^@/types/(.*)$": "<rootDir>/src/types/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.stories.{js,jsx,ts,tsx}",
      "!src/**/index.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{test,spec}.{js,jsx,ts,tsx}"
    ]
  }
}
